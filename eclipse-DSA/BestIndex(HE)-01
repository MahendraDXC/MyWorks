Scanner sc = new Scanner(System.in);
		int N =sc.nextInt();
		int[] an = new int[N];
		for(int i=0;i<N;i++) {
			an[i]=sc.nextInt();
//			System.out.print(an[i]+" ");
		}
		System.out.println();
		int ss[] = new int[N];
		int sum =0;
		for(int j=0;j<N;j++) {
			for(int i=0;i<j;i++) {
				sum+=an[i];
		}
			ss[j]=sum;
		System.out.println(ss[j]);
		}
		
		
		sc.close();

package testdemo;
class A{
	A(){
		
	}
	static{
		System.out.println("class AB loading...............");
	}
	static void m1() {
		System.out.println("class A-m1 loading...............");
	}
}

	

public class demo {
public static void main(String[] args)throws Exception{
	A.m1();
	Class c= Class.forName("testdemo.A");
	Object obj = c.newInstance();
	}
      
	    }
	
package patterns;

public class Pattern1 {

	public static void main(String[] args) {
		System.out.println("\n\t numbers PATTERN\n"+"================================================");
        int i=0,j=0,k=0;
        for(i=0;i<10;i++) {
        	for(j=0;j<20;j++) {
        		k++;
        		System.out.print(k+"  \t");	
        	}
        	System.out.println();
        }
	}

}

package patterns;

public class Pattern1 {

	public static void main(String[] args) {
		System.out.println("\n\t10*10 STAR PATTERN\n"+"================================================");
        int i=0,j=0;
        for(i=0;i<1;i++) {
        	for(j=0;j<12;j++) {
        		System.out.print("* ");
        	}
        }
	}

}

package patterns;

public class StarPattern {

	public static void main(String[] args) {
		System.out.println("\n\t numbers PATTERN\n"+"================================================");
        int i=0;
        char j='A';
        for(i=0;i<10;i++) {
        	for(j='A';j<'Z';j++) {
        		
        		System.out.print(j+" ");	
        	}
        	System.out.println();
        }

	}

}

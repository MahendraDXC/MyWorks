package patterns;

public class Pattern1 {

	public static void main(String[] args) {
		System.out.println("\n\t10*10 STAR PATTERN\n"+"================================================");
        int i=0,j=0;
        for(i=0;i<10;i++) {
        	for(j=0;j<10;j++) {
        		System.out.println("* ");
        	}
        }
	}

}

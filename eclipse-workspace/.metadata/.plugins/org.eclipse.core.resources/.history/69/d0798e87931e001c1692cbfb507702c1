package testdemo;
class A{
	static{
		System.out.println("class AB loading...............");
	}
	static void m1() {
		System.out.println("class A loading...............");
	}
}

	

public class demo {
public static void main(String[] args)throws Exception{
	System.out.println();
	Class.forName("testdemo.A");
	}
      
	    }
	
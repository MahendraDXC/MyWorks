package testdemo;
class A{
	void m1() {
		System.out.println("m1-A");
	}
}
class B extends A{
	void m2(){
		System.out.println("m2-B");
	}
}
	

public class demo {
public static void main(String[] args){
	A a = new A();
	A a1 = new B();
	//B b = (B)a;
	A b = (A)a;
	a1.m1();
	B b1= new B();
	b1.m2();
	b1.m1();
	
	}
      
	    }
	